{
  "name": "japari-admin",
  "version": "1.0.0",
  "description": "",
  "main": "www.js",
  "scripts": {
    "prepare": "husky install",
    "build": "swc src -d built --strip-leading-paths",
    "build:dev": "swc src -d .built-dev --strip-leading-paths --source-maps",
    "start": "npm run build && node ./built/index.js",
    "start:dev": "npm run build:dev && NODE_ENV=dev node ./.built-dev/index.js -p=2333",
    "dev": "swc src -d .built-dev --strip-leading-paths --source-maps --watch",
    "test": "npm run build:dev && NODE_ENV=dev node ./.built-dev/test/index.js",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src"
  },
  "author": "zcWSR",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "overrides": {
    "taffy-pvp-card-sw": {
      "node-fetch": "^2.6.1"
    }
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.84",
    "axios": "^1.13.2",
    "better-sqlite3": "^11.10.0",
    "cfonts": "^3.3.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "es-toolkit": "^1.30.1",
    "firebase-admin": "^12.7.0",
    "html-entities": "^2.6.0",
    "ioredis": "^5.8.2",
    "knex": "^3.1.0",
    "koa": "^2.16.3",
    "koa-body": "^6.0.1",
    "koa-router": "^13.1.1",
    "node-schedule": "^2.1.1",
    "ojsama": "^2.2.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "taffy-pvp-card-sw": "github:muedsa/taffy-pvp-card-sw#0.2.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@swc/cli": "^0.5.0",
    "@swc/core": "^1.10.1",
    "husky": "^9.1.7"
  },
  "repository": "https://github.com/zcWSR/japari-admin"
}
